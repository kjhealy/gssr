citHeader("To cite gssr in publications use:")

bibentry(
  bibtype  = "Manual",
  title    = "gssr: General Social Survey data for use in R",
  author   = person(given = "Kieran",
           family = "Healy",
           role = c("aut", "cre"),
           email = "kieran.healy@duke.edu"),
  year     = "2023",
  note     = "R package version 0.7.0",
  url      = "http://kjhealy.github.io/gssr",
  key      = "gssr"
)
